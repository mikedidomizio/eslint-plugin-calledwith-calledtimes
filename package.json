{
	"name": "eslint-plugin-calledwith-calledtimes",
	"version": "1.4.0",
	"description": "suggest using toHaveBeenCalledTimes when using toHaveBeenCalledWith",
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"toHaveBeenCalledWith",
		"toHaveBeenCalledTimes",
		"toHaveBeenCalledWith toHaveBeenCalledTimes"
	],
	"author": "Mike DiDomizio",
	"main": "./lib/index.js",
	"exports": "./lib/index.js",
	"files": [
		"lib"
	],
	"scripts": {
		"lint": "npm-run-all \"lint:*\"",
		"lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
		"lint:js": "eslint .",
		"test": "mocha tests --recursive",
		"test:watch": "npm run test -- --watch",
		"update:eslint-docs": "eslint-doc-generator",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"stage": "npm run test && npm run prettier:write && npm run lint"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-doc-generator": "^1.0.0",
		"eslint-plugin-eslint-plugin": "^6.0.0",
		"eslint-plugin-n": "^17.0.0",
		"eslint-plugin-prettier": "^5.2.1",
		"mocha": "^10.0.0",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.3.3"
	},
	"engines": {
		"node": "^18.18.0 || ^20.9.0 || >=21.1.0"
	},
	"peerDependencies": {
		"eslint": ">=8.57.0"
	},
	"license": "MIT",
	"prettier": {
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true
	}
}
