{
	"name": "eslint-plugin-calledwith-calledtimes",
	"version": "1.4.2",
	"description": "suggest using toHaveBeenCalledTimes when using toHaveBeenCalledWith",
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"toHaveBeenCalledWith",
		"toHaveBeenCalledTimes",
		"toHaveBeenCalledWith toHaveBeenCalledTimes"
	],
	"author": "Mike DiDomizio",
	"main": "lib/index.ts",
	"exports": "./dist/index.js",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "tsup lib/index.ts --format cjs --out-dir dist",
		"lint": "npm-run-all \"lint:*\"",
		"lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
		"lint:js": "eslint .",
		"test": "vitest --run --hideSkippedTests",
		"test:watch": "vitest",
		"update:eslint-docs": "eslint-doc-generator",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"stage": "npm run test && npm run prettier:write && npm run lint",
		"ts": "tsc --noEmit"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@types/eslint": "^9.6.1",
		"@types/estree": "^1.0.8",
		"@typescript-eslint/parser": "^8.46.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-doc-generator": "^1.0.0",
		"eslint-plugin-eslint-plugin": "^6.0.0",
		"eslint-plugin-n": "^17.0.0",
		"eslint-plugin-prettier": "^5.2.1",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.3.3",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": "^18.18.0 || ^20.9.0 || >=21.1.0"
	},
	"peerDependencies": {
		"eslint": ">=8.57.0"
	},
	"license": "MIT",
	"prettier": {
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true
	}
}
